{
	"name": "@dukkani/auth",
	"version": "0.1.1",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.ts"
		},
		"./env": {
			"types": "./dist/env.d.ts",
			"default": "./src/env.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"default": "./src/*.ts"
		}
	},
	"type": "module",
	"scripts": {
		"build": "tsdown",
		"clean": "rm -rf dist node_modules"
	},
	"devDependencies": {
		"@dukkani/config": "workspace:*",
		"tsdown": "catalog:"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@dukkani/db": "workspace:*",
		"@dukkani/env": "workspace:*",
		"@dukkani/common": "workspace:*",
		"@t3-oss/env-core": "catalog:",
		"better-auth": "catalog:",
		"dotenv": "catalog:",
		"zod": "catalog:"
	}
}
